{
    "version": 2,
    "builds": [
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "styling.css",
        "use": "@vercel/static"
      },
      {
        "src": "Pictures/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/Home.html"
      },
      {
        "src": "/request",
        "dest": "/Request.html"
      },
      {
        "src": "/status",
        "dest": "/Status.html"
      },
      {
        "src": "/(.*)",
        "dest": "/Home.html"
      }
    ]
  }
  